
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="container">
    <div id="preloader" style="display: none;">
        <div class="overlay">
            <p>
                Loading
                <span class="spinner"></span>
            </p>
        </div>
    </div>

    <div id="dvMain" class="panel panel-default">

        <div class="panel-body">
            <div class="form-group">


                <div class="row" style="padding-bottom: 20px;">
                    <div class="col-md-12">
                        <label class="Label col-sm-2">Upload file :<span class="red-text "> *</span></label>

                        <div class="col-md-4">
                            <label id="lblflvRecoUpload" class="btn btn-red btn-medium btn-bs-file btn btn-lg btn-primary " style="float: left; margin-right: 10px;">
                                Browse File
                                <input type="file" id="flvRecoUpload" accept="application/vnd.ms-excel,application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" class="KYC" style="display: none" />
                            </label>

                            <div id="dvfiletemplate" style="float: left">
                                <a id="atemplate" href="~/Templates/HDFC_EXCEL_UPLOAD.xls" class="btn btn-red btn-small" style="height: 30px;">
                                    <img src="~/img/download-button.png" style="height: 17px; margin: 5px;" />
                                </a>
                            </div>
                            <br />

                            <label id="lblRecoUpload" class="Label"></label>

                        </div>
                    </div>
                </div>
                <div class="row" style="padding-bottom: 6px;">
                    <div class="col-md-6">
                        <label class="Label col-sm-4">Remarks :<span class="red-text "> *</span></label>
                        <div class="col-sm-8" style="padding-bottom: 6px;">
                            <textarea id="txtRemarks" class="form-control" rows="3" cols="10"></textarea>
                            <span class="red-text help-block"></span>
                        </div>
                    </div>
                </div>
                <div class="row" style="padding-bottom: 6px;">
                    <div class="col-sm-12" style="padding-top: 12px;">
                        <button class="btn btn-red" type="button" id="btnProcess">Process</button>
                        <button class="btn btn-red" type="button" id="btnCancel">Cancel</button>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
<script src="~/AppJs/HDFCSoftFeedReconciliation.js" asp-append-version="true"></script>